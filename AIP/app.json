{
  "expo": {
    "name": "GraalTest",
    "slug": "graaltest",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
